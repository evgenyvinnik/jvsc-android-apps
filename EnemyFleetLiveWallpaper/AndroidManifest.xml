				 android:icon="@drawable/icon"
				 android:thumbnail="@drawable/icon">
			<intent-filter>
				<action android:name="android.service.wallpaper.WallpaperService" />
			</intent-filter>
			<meta-data android:name="android.service.wallpaper"
					   android:resource="@xml/enemy_fleet" />
		</service>


